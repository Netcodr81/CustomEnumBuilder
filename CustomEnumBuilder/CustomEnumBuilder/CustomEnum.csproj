    <RepositoryUrl>https://github.com/Netcodr81/CustomEnumBuilder</RepositoryUrl>
    <Version>1.2.0</Version>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\OneDrive\NetcodrIcon128px.png">
