<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>CustomEnumBuilder</PackageId>
    <Authors>Mike Eckel</Authors>
    <Company>NetCodr</Company>
    <Product />
    <Description>This class library allows users to create custom enum classes. This allows users to assign a value other than an integer.</Description>
    <AssemblyVersion>1.2.0.0</AssemblyVersion>
    <FileVersion>1.2.0.0</FileVersion>
    <PackageIcon>NetcodrIcon128px.png</PackageIcon>
    <PackageIconUrl />
